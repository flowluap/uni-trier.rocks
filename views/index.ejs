     <div class="container box-container">
    <% for(i in events){ %>

 		<div class="row">
 			<div class="col-3 text-right">
 				<h1 class="display-4"><span class="badge badge-<%= (events[i].event.prio=='low' ? 'secondary' : 'danger') %>"><%= events[i].date %></span></h1>
 				<h2><%= events[i].month %></h2>
 			</div>
 			<div class="col-8">
 				<h3 class="text-uppercase"><strong><%= events[i].event.task %></strong></h3>
 				<ul class="list-inline">
 				    <li class="list-inline-item"><i class="fa fa-calendar-o" aria-hidden="true"></i> <%= events[i].day %></li>

 					<li class="list-inline-item"><i class="fa fa-location-arrow" aria-hidden="true"></i> Uni</li>
 				</ul>
 				<p><%= events[i].event.note %></p>
 			</div>
 		</div>
    <% } %>
 	</div>

  <div class="container box-container">
